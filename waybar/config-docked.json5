[{
    "output": "HDMI-A-2",
     "layer": "top", // Waybar at top layer
    // "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 24, // Waybar height (to be removed for auto height)
    // "width": 1280, // Waybar width
    "spacing": 5, // Gaps between modules (4px)
    "margin-left": 5,
    "margin-right": 5,
    "margin-bottom": 0,
    "margin-top": 5,
    // Choose the order of the modules
    "modules-left": ["hyprland/workspaces","hyprland/window"],
    "modules-center": [],
    "modules-right": ["tray", "pulseaudio#out", "pulseaudio#in", "clock", "clock#time"],
    // Modules configuration
    // "sway/workspaces": {
    //     "disable-scroll": true,
    //     "all-outputs": true,
    //     "warp-on-scroll": false,
    //     "format": "{name}: {icon}",
    //     "format-icons": {
    //         "1": "",
    //         "2": "",
    //         "3": "",
    //         "4": "",
    //         "5": "",
    //         "urgent": "",
    //         "focused": "",
    //         "default": ""
    //     }
    // },
    "hyprland/workspaces": {
        "persistent_workspaces": {
            "*": 5,
        },
    },
    "tray": {
        "show-passive-items": true,
        // "icon-size": 21,
        "spacing": 10
    },
    "clock": {
        "interval": 600,
        // "timezone": "America/New_York",
        "format": "{:  %A, %B %d %Y}",
    },
    "clock#time": {
        "interval": 1,
        // "timezone": "America/New_York",
        "format": "{: %H:%M:%S}",
    },
    "pulseaudio#out": {
        // "scroll-step": 1, // %, can be a fcoat
        "format": "{icon} {volume}%",
        "format-bluetooth": "{volume}% {icon}",
        "format-bluetooth-muted": "  Muted",
        "format-muted": "  Muted",
        "format-icons": {
            "headphone": " ",
            "hands-free": "HF  ",
            "headset": " ",
            "phone": " ",
            "portable": " ",
            "car": "  ",
            "default": [" ", " ", " "]
        },
        "on-click": "pavucontrol"
    },
    "pulseaudio#in": {
        // "scroll-step": 1, // %, can be a fcoat
        "format": "{format_source}",
        "format-bluetooth": "{format_source}",
        "format-bluetooth-muted": "{format_source}",
        "format-muted": "{format_source}",
        "format-source": " {volume}%",
        "format-source-muted": " ",
        "on-click": "pavucontrol"
    },
},
{
    "output": "HDMI-A-2",
    // "layer": "top", // Waybar at top layer
    "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 24, // Waybar height (to be removed for auto height)
    // "width": 1280, // Waybar width
    "spacing": 5, // Gaps between modules (4px)
    "margin-left": 5,
    "margin-right": 5,
    "margin-top": 0,
    "margin-bottom": 5,
    // Choose the order of the modules
    "modules-left": ["network", "backlight"],
    "modules-center": [],
    "modules-right": ["battery#internal", "battery#external", "temperature#cpu", "cpu", "memory"],
    // Modules configuration
    // "sway/workspaces": {
    //     "disable-scroll": true,
    //     "all-outputs": true,
    //     "warp-on-scroll": false,
    //     "format": "{name}: {icon}",
    //     "format-icons": {
    //         "1": "",
    //         "2": "",
    //         "3": "",
    //         "4": "",
    //         "5": "",
    //         "urgent": "",
    //         "focused": "",
    //         "default": ""
    //     }
    // },
    "backlight": {
	"format": " {percent}%"
    },
    "tray": {
        // "icon-size": 21,
        "spacing": 10
    },
    "cpu": {
        "interval": 1,
        "format": " {usage}%",
        "tooltip": false
    },
    "memory": {
        "interval": 1,
        "format": " {}%"
    }, 
    "temperature#cpu": {
        
        "interval": 1,
        "thermal-zone": 1,
        "hwmon-path": "/sys/devices/platform/thinkpad_hwmon/hwmon/hwmon5/temp1_input",
        "critical-threshold": 90,
        "format-critical": "CPU {icon}{temperatureC}°C",
        "format": "CPU {icon}{temperatureC}°C",
        "format-icons": [" ", " ", " "]
    },
    "temperature#gpu": {
        
        "interval": 1,
        "thermal-zone": 0,
        "hwmon-path": "/sys/devices/pci0000:00/0000:00:01.0/0000:01:00.0/0000:02:00.0/0000:03:00.0/hwmon/hwmon2/temp2_input",
        "critical-threshold": 75,
        "format": "GPU {icon}{temperatureC}°C",
        "format-critical": "GPU {icon}{temperatureC}°C",
        "format-icons": [" ", " ", " "]
    },
    "battery#internal": {
        "bat": "BAT0",
	"interval": 1,
	"format-time": "{H}h{m}m",
	"format": "  int {capacity}% ({time})",
    },
    "battery#external": {
        "bat": "BAT1",
	"interval": 1,
	"format-time": "{H}h{m}m",
	"format": "  ext {capacity}% ({time})",
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "  {essid} ({signalStrength}%)",
        "format-ethernet": "  {ipaddr}/{cidr}",
        "tooltip-format": "  {ifname} via {gwaddr}",
        "format-linked": "  {ifname} (No IP)",
        "format-disconnected": "⚠ Disconnected",
        //"format-alt": "{ifname}: {ipaddr}/{cidr}"
    },
},
